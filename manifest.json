{
  "manifest_version": 3,
  "name": "Sample TypeScript Extension",
  "version": "1.0",
  "permissions": ["tabs", "activeTab"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*",
        "https://localhost/*",
        "https://furuyoni-simulator.herokuapps.com/*"
      ],
      "js": ["dist/content.js"]
    }
  ]
}
